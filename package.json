{
  "name": "@k-art/masonry-grid",
  "version": "1.0.2",
  "description": "Masonry grid layout",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "keywords": [
    "react",
    "masonry",
    "grid",
    "layout",
    "ui",
    "component"
  ],
  "author": {
    "name": "Kirill Art",
    "url": "https://github.com/curillaenator"
  },
  "license": "ISC",
  "scripts": {
    "build": "npx rimraf dist && npx yarn build:ts && npx yarn build:scss && npx yarn build:fix",
    "build:ts": "npx tsc -p tsconfig.json",
    "build:scss": "npx sass --no-source-map --update src:dist",
    "build:fix": "bash ./bin/fixImports.sh"
  },
  "dependencies": {
    "classnames": "2.5.1"
  },
  "peerDependencies": {
    "lodash": "*",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.14",
    "@types/react": ">=18.2.0",
    "@types/react-dom": ">=18.2.0",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/curillaenator/masonry-grid.git"
  },
  "bugs": {
    "url": "https://github.com/curillaenator/masonry-grid/issues"
  },
  "homepage": "https://github.com/curillaenator/masonry-grid#readme"
}
