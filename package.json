{
  "name": "@k-art/masorny",
  "version": "1.0.0",
  "description": "Masorny grid layout",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "React",
    "Masorny",
    "Grid"
  ],
  "author": "https://github.com/curillaenator",
  "license": "ISC",
  "scripts": {
    "build": "npx rimraf dist && npx yarn build:ts && npx yarn build:scss && npx yarn build:fix",
    "build:ts": "npx tsc -p tsconfig.json",
    "build:scss": "npx sass --no-source-map --update src:dist",
    "build:fix": "bash ./bin/fixImports.sh"
  },
  "dependencies": {
    "classnames": "2.5.1"
  },
  "peerDependencies": {
    "lodash": "*",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.14",
    "@types/react": ">=18.2.0",
    "@types/react-dom": ">=18.2.0",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@3.6.0"
}
